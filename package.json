{
  "name": "abd-stream",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:server": "node server/server.mjs",
    "dev:media": "node server/simple-media.mjs",
    "dev:full": "concurrently \"npm run dev:server\" \"npm run dev:media\" \"npm run dev\" --names \"WebSocket,Media,Vite\" --prefix-colors \"cyan,yellow,magenta\"",
    "start:all": "concurrently \"npm run dev:server\" \"npm run dev\" \"npm run dev:media\" --names \"WebSocket,Vite,MediaServer\" --prefix-colors \"cyan,magenta,yellow\"",
    "dev:rtmp": "node server/simple-rtmp.mjs"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "lucide-react": "^0.468.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10"
  }
}